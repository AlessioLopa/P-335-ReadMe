<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="P_365_ReadME.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:P_365_ReadME"
    Shell.FlyoutBehavior="Disabled"
    Title="P_365_ReadME">

    <Tab Title="BookPage">
        <ShellContent
        ContentTemplate="{DataTemplate local:ApiPage}"
        Route="ApiPage" />
    </Tab>

    <Tab Title="BookList">
        <ShellContent
            ContentTemplate="{DataTemplate local:MainPage}"
            Route="MainPage" />
    </Tab>

</Shell>
